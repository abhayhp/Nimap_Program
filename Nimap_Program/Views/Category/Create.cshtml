<h2> Create </h2>


@using (Html.BeginForm())
{
    <div class="form-horizontal">
        <div class="form-group">
            <label>Category Id</label>
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.CategoryID)

            </div>
        </div>

        <div class="form-group">
            <label>Category Name</label>
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.CategoryName)

